  "keywords": [
    "binary",
    "flags",
    "class",
    "trait",
    "eloquent",
    "model",
    "bitwise",
    "bitwiser",
    "operations"
  ],
  "homepage": "https://github.com/reinder83/binary-flags",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Reinder Reinders",
      "email": "reinder.reinders@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
