    "sebastian/phpcpd": "^3.0",
    "squizlabs/php_codesniffer": "^3.4",
    "phpunit/phpunit": "^5.7",
    "phploc/phploc": "^4.0",
    "pdepend/pdepend": "^2.5",
    "mayflower/php-codebrowser": "^1.1",
